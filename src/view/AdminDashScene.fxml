<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminController">

    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Admin Dashboard" />
        <ListView layoutX="14.0" layoutY="50.0" prefHeight="300.0" prefWidth="200.0" fx:id="users" />
        <Button layoutX="250.0" layoutY="85.0" mnemonicParsing="false" onAction="#handleCreateUserButtonAction" text="Add User" />
        <Button layoutX="250.0" layoutY="120.0" mnemonicParsing="false" onAction="#handleDeleteUserButtonAction" text="Delete User" />
        <Button layoutX="250.0" layoutY="155.0" mnemonicParsing="false" onAction="#handleLogoutButtonAction" text="Logout" />
    </children>
</AnchorPane>
